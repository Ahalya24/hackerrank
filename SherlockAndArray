def check_ele(arr_i,n_ele):
    arr_s=sorted(arr_i)
    for i in range(1,n_ele):
        if sum(arr_s[:i-1])==sum(arr_s[i:]):
            x='YES'
        else:
            x='NO'
    print (x)


n_test=int(input())
for i in range(n_test):
    n_ele=int(input())
    arr=input().split()
    arr_i=[int(a) for a in arr]
    check_ele(arr_i,n_ele)
